{
	"env": {
		"browser": true,
		"node": true
	},
	"extends": ["next", "next/core-web-vitals", "prettier"],
	/// Start typescript
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint"],
	"settings": {
		"import/extensions": [".js", ".jsx", ".ts", ".tsx"],
		"import/parsers": {
			"@typescript-eslint/parser": [".ts", ".tsx"]
		},
		"import/resolver": {
			"node": {
				"extensions": [".js", ".jsx", ".ts", ".tsx"]
			}
		}
	}
}
